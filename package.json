{
  "name": "bachata-science",
  "version": "1.0.0",
  "description": "A catalog of the most common bachata dance steps",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean:build && webpack --mode production",
    "clean:build": "rm -rf dist",
    "clean:dependencies": "rm -rf node_modules/ package-lock.json",
    "start": "node index.js",
    "start:dev": "webpack-dev-server --open --mode development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/L3bowski/bachata-science"
  },
  "keywords": [
    "bachata",
    "science"
  ],
  "author": "Carles Capellas <capellas.carles@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "4.17.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "babel-loader": "8.0.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
